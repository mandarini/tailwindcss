{
  "targets": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/build.rs", "{projectRoot}/package.json", "{projectRoot}/Cargo.toml"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/build.rs", "{projectRoot}/package.json", "{projectRoot}/Cargo.toml"],
      "cache": false,
      "persistent": true
    }
  }
}
